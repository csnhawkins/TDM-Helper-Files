{
  "jsonSchemaVersion": 1,
  "howTo": "Example logic can be found below, some of which commented out. Simply remove \\ at the beginning of the attribute name to activate. For example '//excludedTables' becomes 'excludedTables'",
  "tables": [
    {
      "schema": "public",
      "name": "customer",
      "exclude": false,
      "columns": [
        {
          "name": "email",
          "dataset": "CustomEmailAddresses",
          "deterministic":false
        },
        {
          "name": "city",
          "dataset": "CustomCities",
          "deterministic": false,
          "exclude": false,
          "comment": "This column uses the CustomCities conditional dataset to select a city name based on the value of the Country field"
        } 
      ]
    }
  ],
  "datasets": [
    {
      "name": "CompanyNames",
      "type": "List",
      "values": [
        "Redgate Software",
        "Microsoft",
        "Google",
        "Amazon",
        "Apple",
        "Facebook",
        "Twitter"
      ]
    },
    {
      "name": "CustomEmailAddresses",
      "type": "Expression",
      "expression": "$(GivenNames($[first_name])).$(FamilyNames($[last_name]))@$(Domains)",
      "description": "Utilize value from masked FirstName and LastName column and concat with value from Domains dataset"
    },
    {
      "name": "Domains",
      "type": "List",
      "values": [
        "gmail.com",
        "outlook.com",
        "hotmail.com",
        "icloud.com",
        "protonmail.com",
        "mail.com",
        "aol.com"
      ]
    },
    {
      "name": "CustomCities",
      "type": "Conditional",
      "comment": "A conditional dataset that selects a city list based on the value of the 'Country' field. Defaults to 'Cities' if no match is found.",
      "conditions": [
        { "if": "$[country] == \"United Kingdom\"", "then": "UKCities" },
        { "if": "$[country] == \"USA\"", "then": "USCities" },
        { "otherwise": "Cities" }
      ]
    },
    {
      "name": "UKCities",
      "type": "File",
      "file": "UKCities.txt",
      "comment": "A file-based dataset referencing a list of UK cities from a local .txt file."
    },
    {
      "name": "USCities",
      "type": "File",
      "file": "USCities.txt",
      "comment": "A file-based dataset referencing a list of US cities from a local .txt file."
    }, 
    {
      "name": "JobTitles",
      "type": "List",
      "values": [
        "CEO",
        "CTO",
        "Director",
        "Engineer",
        "Developer",
        "Designer",
        "Analyst",
        "Consultant"
      ]
    },
    {
      "name": "Websites",
      "type": "List",
      "values": [
        "https://www.example.com",
        "https://www.test.com",
        "https://www.google.com"
      ]
    }
  ],
  "settings": {
    "dateTimeFormats": [
      "YYYY-MM-dd",
      "yyyy-mm-dd HH:mm:ss"
    ]
  },
  "classifications": {
    "builtIn": {
      "enabled": [ 
         "Cities",
         "CreditCardNumbers",
         "DatesOfBirth",
         "EmailAddresses",
         "FullNames",
         "FamilyNames",
         "GivenNames",
         "IPAddresses",
         "PassportNumbers",
         "PhoneNumbers",
         "PostCodes",
         "StreetAddresses",
         "StreetAddresses",
         "UKCounties",
         "UKNationalInsuranceNumbers",
         "USSocialSecurityNumbers"
       ],
       "disabled": [ 
        "Countries",
        "ZipCodes" 
      ]
    },
    "custom": [
      {
        "type": "CompanyNames",
        "confidence": "High",
        "condition": "Column.Name contains 'Company' AND Column.Name contains 'Name'"
      }
    ]
  }
}